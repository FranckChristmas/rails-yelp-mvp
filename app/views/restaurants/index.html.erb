<div class="position-relative">
<h1>RESTAURANT INDEX<h1>
<div class="m-5">
<%= link_to "+ New Restaurant", new_restaurant_path, class: "btn btn-primary"  %>
</div>
<% @restaurants.each do |restaurant|%>
<div class="m-5">
<ul class="list-unstyled">
<li class="list-group-item"><h3><%= link_to restaurant.name, restaurant_path(restaurant) %></h3>
<h6><%= restaurant.category%></h6>
<h6><%= restaurant.address%></h6>
</li>
</ul>
</div>
<% end %>
</div>



